@model IEnumerable<AppUser>

@{
    ViewBag.Title = "Kullanıcı Listesi";
}

<h2>Kullanıcı Listesi</h2>

<table class="table">
    <thead>
        <tr>
            <th>Kullanıcı Adı</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>


